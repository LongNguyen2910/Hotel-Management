@model IEnumerable<Hotel_Management.Models.Chucvu>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_ContentNavbarLayout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Tenchucvu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Luongcoban)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Tenchucvu)
                </td>
            <td>
                @Html.DisplayFor(modelItem => item.Luongcoban)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Tenchucvu">Edit</a> 
                <a asp-action="Delete" asp-route-id="@item.Tenchucvu">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
